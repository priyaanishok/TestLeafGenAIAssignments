{
  "name": "user-story-to-tests",
  "version": "1.0.0",
  "description": "Full-stack application that converts user stories into test cases using LLM integration",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=backend\" \"npm run dev --workspace=frontend\"",
    "typecheck": "npm run typecheck --workspaces"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "concurrently": "^8.2.2",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "ts-jest": "^29.4.1"
  },
  "dependencies": {
    "@types/form-data": "^2.2.1",
    "@types/multer": "^2.0.0",
    "form-data": "^4.0.4",
    "multer": "^2.0.2"
  }
}
